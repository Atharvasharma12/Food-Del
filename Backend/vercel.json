 {
    "version" : 2,
    "builds" : [
        {
            "src" : "./index.js",
            "use" : "@vercel/node"
        }
    ],
    "routes" : [
        {
            "src" : "/(.*)",
            "dest" : "/index.js",
            "methods":["GET", "POST", "PUT", "DELETE", "PUT", "PATCH", "OPTIONS"]
        }
    ]
 }